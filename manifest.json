{
    "manifest_version": 3,
    "name": "QR Generator Extension",
    "version": "1.0",
    "action": {
      "default_popup": "index.html",
      "default_icon": {
        "16": "qr.png",
        "48": "qr.png",
        "128": "qr.png"
      }
    },
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "content_security_policy": {
      "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self'"
    }
  }

